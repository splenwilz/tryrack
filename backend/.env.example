# WorkOS Configuration
# Get these from your WorkOS Dashboard: https://dashboard.workos.com/api-keys
WORKOS_API_KEY=sk_example_123456789
WORKOS_CLIENT_ID=client_123456789

# Session Management
# Generate a secure random string for cookie encryption
WORKOS_COOKIE_PASSWORD=your_secure_cookie_password_here

# Redirect URI for WorkOS callbacks
# This should match what you configure in your WorkOS Dashboard
WORKOS_REDIRECT_URI=http://localhost:8000/auth/callback

# Frontend URL for redirects after authentication
FRONTEND_URL=http://localhost:3000
